<select class="selectpicker form-control" name="property_type">
    <option value="" selected disabled>Tipul Proprietății</option>
    <% for(let prop of propertyTypes) {
        var selected = (prop.value) == property_type ? "selected" : "";
    %>
    <option value="<%=prop.value%>" <%= selected %>><%= prop.display %></option>
    <% } %>
</select>
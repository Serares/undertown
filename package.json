{
  "name": "undertown",
  "version": "1.0.0",
  "description": "Real estate webapp",
  "engines": {
    "node": "14.x"
  },
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm-run-all clean build-ts copy-static-assets parcel-build-listings parcel-build-public parcel-build-public-auth",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "parcel-build-listings": "parcel build client/listings/index.tsx -d dist/public/js/custom/listings",
    "parcel-watch-listings": "parcel watch client/listings/index.tsx -d dist/public/js/custom/listings",
    "parcel-build-public": "parcel build src/public/js/custom/*.ts -d dist/public/js/custom",
    "parcel-watch-public": "parcel watch src/public/js/custom/*.ts -d dist/public/js/custom",
    "parcel-build-public-lib": "parcel build src/public/js/lib/*.js -d dist/public/js/lib/",
    "parcel-watch-public-auth": "parcel watch src/public/js/auth/main.ts -d dist/public/js/auth/",
    "parcel-build-public-auth": "parcel build src/public/js/auth/main.ts -d dist/public/js/auth/",
    "test": "jest --forceExit --coverage --verbose",
    "test-cloud": "jest --forceExit",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "gcp-build": "npm run build",
    "create-env": "printenv > .env"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.3",
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@sendgrid/mail": "^7.4.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "ejs": "^2.5.6",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "helmet": "^3.22.0",
    "http-proxy-middleware": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.22",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reactstrap": "^8.9.0",
    "serve-static": "^1.14.1",
    "vue": "^2.6.12",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.15",
    "@types/compression": "^1.7.0",
    "@types/eslint": "^7.2.7",
    "@types/express": "^4.17.11",
    "@types/express-validator": "^3.0.0",
    "@types/faker": "^5.1.7",
    "@types/helmet": "^4.0.0",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^26.0.20",
    "@types/jquery": "^3.5.5",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/nanoid": "^2.1.0",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/serve-static": "^1.13.9",
    "@types/shelljs": "^0.8.8",
    "@types/supertest": "^2.0.10",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chai": "^4.3.3",
    "eslint": "^7.21.0",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.3",
    "tape": "^4.7.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Serares/undertown.git"
  },
  "keywords": [
    "NodeJS",
    "Typescript"
  ],
  "author": "Rares Jianu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Serares/undertown/issues"
  },
  "homepage": "https://github.com/Serares/undertown#readme"
}
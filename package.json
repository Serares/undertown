{
  "name": "undertown",
  "version": "1.0.0",
  "description": "Real estate webapp",
  "engines": {
    "node": "12.x"
  },
  "main": "dist/server.js",
  "scripts": {
    "build": "npm-run-all build-ts copy-static-assets parcel",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "parcel": "parcel build src/public/js/custom/*.ts -d dist/public/js/custom",
    "parcel-watch": "parcel watch src/public/js/custom/*.ts -d dist/public/js/custom",
    "test": "jest --forceExit --coverage --verbose",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "gcp-build": "npm run build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "ejs": "^2.5.6",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.10.0",
    "helmet": "^3.22.0",
    "http-proxy-middleware": "^1.0.6",
    "multer": "^1.4.2",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "serve-static": "^1.14.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.15",
    "@types/compression": "^1.7.0",
    "@types/connect-flash": "0.0.35",
    "@types/eslint": "^7.2.7",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/express-validator": "^3.0.0",
    "@types/faker": "^5.1.7",
    "@types/helmet": "^4.0.0",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^26.0.20",
    "@types/jquery": "^3.5.5",
    "@types/multer": "^1.4.5",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/serve-static": "^1.13.9",
    "@types/shelljs": "^0.8.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chai": "^4.3.3",
    "eslint": "^7.21.0",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.3",
    "tape": "^4.7.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Serares/undertown.git"
  },
  "keywords": [
    "NodeJS",
    "Typescript"
  ],
  "author": "Rares Jianu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Serares/undertown/issues"
  },
  "homepage": "https://github.com/Serares/undertown#readme"
}
